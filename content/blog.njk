---js
const eleventyNavigation = {
key: "Archive",
order: 2
};
---
<!DOCTYPE html>
<html>

<head>
	<link href="/css/blog.css" rel="stylesheet" />
</head>

<div class="posts">
	{% set postslist = collections.posts | head(-1 * 9999) %}
	{% set postslistCounter = postsCount %}
	{%- css %}.postlist { counter-reset: start-from {{ (postslistCounter or postslist.length) + 1 }} }{%
	endcss %}
	{%- for post in postslist | reverse %}
	<div class="blogname">
		<ul reversed class="latestlist">
			<div class="latestlistitem">
				<div class="postlist-link"><a href="{{ post.url }}" class="postlist-link"> <span class="moe"></span>{% if post.data.title %}{{ post.data.title }}{% else %}{% endif %}</a></div>
				<time class="postlist-date" datetime="{{ post.date | htmlDateString }}">{{ post.date | readableDate(" DDDD ")
				}}</time>
			</div>
		</ul>
	</div>
	<div class="blogtext">
		<p>
			{{post.templateContent | excerpt1 }}
		</p>

		{%- endfor %}
	</div>

	<img class="yana" src="/ayanscii22.png" alt="yaneth">