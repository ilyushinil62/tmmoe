---js
const eleventyNavigation = {
key: "Home",
order: 1
};

---
<!DOCTYPE html>
<html>

<head>
    <link href="/css/index.css" rel="stylesheet" />
</head>

<body>
    <div class="container">
        <div class="main">
            <div class="blogname">{% set postslist = collections.posts | head(-1 * 1) %}
                {% set postslistCounter = postsCount %}
                {%- css %}.postlist { counter-reset: start-from {{ (postslistCounter or postslist.length) + 1 }} }{%
                endcss %}
                <ul reversed class="latestlist">
                    {%- for post in postslist | reverse %}
                    <li class="latestlistitem">
                        <div class="postlist-link">{% if post.data.title %}{{ post.data.title }}{% else %}{% endif %}
                        </div>
                    </li>


                </ul>
            </div>
            <div class="blogtext">
                <p>
                    {{post.templateContent | excerpt1 }}
                </p>
                want <a href="{{ post.url }}" class="postlist-link"> <span class="moe">more</span>
                </a>?

                {%- endfor %}

            </div>
            <div class="other">

                <div class="dumbbox">
                    <div class="latestblog">
                        <ul class="latestlist">
                            {% set postslist = collections.posts | head(-1 * 5) %}
                            {% set postslistCounter = postsCount %}
                            {% include "postslist.njk" %}

                            {# List every content page in the project #}
                            {#
                            <ul>
                                {%- for entry in collections.all %}
                                <li><a href="{{ entry.url }}"><code>{{ entry.url }}</code></a></li>
                                {%- endfor %}
                            </ul>
                            #}

                        </ul>
                    </div>
                    <div class="warning">

                    </div>
                </div>
            </div>
        </div>
        <div class="name">
            <div class="headertext">
                trademark.moe trademark.moe trademark.<span class=moe>moe</span>
            </div>
        </div>
        <div class="nav">
            <ul class="navlist">
                <li class="navitem">who</li>
                <li class="navitem">journal</li>
                <li class="navitem">friends</li>
                <li class="navitem">marking</li>
                <li class="navitem">watchlist</li>
            </ul>
        </div>
    </div>

</body>
</html>